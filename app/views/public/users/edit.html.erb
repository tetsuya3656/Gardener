<h1>プロフィール編集</h1>
  <%= form_with model: @user do |f| %>
    <label for="inputName">ユーザーネーム</label>
      <%= f.text_field :name, autofocus: true, id:"inputName",placeholder:"新しいユーザーネーム" %>
    <label for="inputImage">プロフィール画像</label>
      <%= f.file_field :profile_image, placeholder: "新しいプロフィール画像", accept:"image/*" %>
    <label for="inputAddress">お住まい</label>
      <%= f.select :address, User.addresses.keys, {prompt: '選択してください'} %>
    <%= f.submit '編集内容を保存' %>
  <% end %>     

  <%= link_to "退会する", unsubscribe_path %>